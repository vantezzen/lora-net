{
  "name": "lora-net",
  "version": "1.0.0",
  "description": "Console App for building an adhoc, multihop network using LoRa wireless modules",
  "main": "index.js",
  "scripts": {
    "build": "tsc -p .",
    "start": "ts-node src/main.ts",
    "watch": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/main.ts",
    "package-test": "ts-node src/packageTest.ts",
    "virtual-net": "DEBUG=lora:* nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/virtual/main.ts",
    "tcp-net": "DEBUG=lora:* ts-node src/tcp/main.ts",
    "tcp-server": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/tcp/server.ts",
    "web:start": "cd src/web-interface && react-scripts start",
    "web:build": "cd src/web-interface && react-scripts build",
    "web:server": "DEBUG=lora:* ts-node src/webserver.ts",
    "web:dev:server": "nodemon",
    "web:dev": "concurrently \"npm:web:dev:server\" \"npm:web:start\""
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@types/yargs": "^17.0.7",
    "chalk": "^4.1.2",
    "cli-diagram": "^0.3.0",
    "cli-progress": "^3.9.1",
    "cli-table": "^0.3.9",
    "debug": "^4.3.3",
    "glob": "^7.2.0",
    "inquirer": "^8.2.0",
    "serialport": "^9.2.8",
    "yargs": "^17.3.0",
    "@supabase/ui": "^0.36.2",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.5",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "concurrently": "^6.5.1",
    "cors": "^2.8.5",
    "express": "^4.17.2",
    "javascript-time-ago": "^2.3.10",
    "nodemon": "^2.0.15",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-feather": "^2.0.9",
    "react-scripts": "5.0.0",
    "socket.io": "^4.4.0",
    "socket.io-client": "^4.4.0",
    "ts-node": "^10.4.0",
    "web-vitals": "^2.1.2"
  },
  "devDependencies": {
    "@types/cli-progress": "^3.9.2",
    "@types/cli-table": "^0.3.0",
    "@types/glob": "^7.2.0",
    "@types/inquirer": "^8.1.3",
    "@types/node": "^16.11.11",
    "@types/serialport": "^8.0.2",
    "nodemon": "^2.0.15",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.2",
    "autoprefixer": "^10.4.1",
    "postcss": "^8.4.5",
    "tailwindcss": "^3.0.8"
  }
}
